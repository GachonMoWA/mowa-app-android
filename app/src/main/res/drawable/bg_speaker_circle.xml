<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="45"
                android:endColor="#0AA66D"
                android:startColor="#330AA66D" />
            <corners android:radius="7dp" />
        </shape>

    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <solid android:color="#ffffff" />
            <corners android:radius="7dp" />
        </shape>

    </item>
</layer-list>
